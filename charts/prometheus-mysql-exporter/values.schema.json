{
    "$schema": "https://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "namespaceOverride": {
            "type": "string",
            "description": "Override the namespace which the resources will be deployed in"
        },
        "fullnameOverride": {
            "type": "string",
            "description": "Override release name"
        },
        "replicaCount": {
            "type": "integer",
            "description": "Number of replicas"
        },
        "image": {
            "type": "object",
            "properties": {
                "registry": {
                    "type": "string",
                    "description": "Image registry"
                },
                "repository": {
                    "type": "string",
                    "description": "Image repository"
                },
                "tag": {
                    "type": "string",
                    "description": "Image tag, if not set charts appVersion var is used"
                },
                "pullPolicy": {
                    "type": "string",
                    "description": "Image pull policy",
                    "enum": [
                        "Never",
                        "IfNotPresent",
                        "Always"
                    ]
                }
            }
        },
        "imagePullSecrets": {
            "type": "array",
            "description": "Image pull secrets",
            "items": {
                "type": "object"
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "labels": {
                    "type": "object",
                    "description": "Service labels",
                    "additionalProperties": true
                },
                "annotations": {
                    "type": "object",
                    "description": "Service annotations",
                    "additionalProperties": true
                },
                "name": {
                    "type": "string",
                    "description": "Service name"
                },
                "type": {
                    "type": "string",
                    "description": "Service type",
                    "enum": [
                        "ClusterIP",
                        "NodePort",
                        "LoadBalancer",
                        "ExternalName"
                    ]
                },
                "externalPort": {
                    "type": "integer",
                    "description": "External port"
                },
                "internalPort": {
                    "type": "integer",
                    "description": "Internal port"
                }
            }
        },
        "serviceMonitor": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "description": "Enable prometheus-operator discovery of this service"
                },
                "apiVersion": {
                    "type": "string",
                    "description": "API version for ServiceMonitor"
                },
                "interval": {
                    "type": "string",
                    "description": "Interval at which metrics should be scraped"
                },
                "scrapeTimeout": {
                    "type": "string",
                    "description": "Timeout after which the scrape is ended"
                },
                "namespace": {
                    "type": "string",
                    "description": "Namespace for ServiceMonitor"
                },
                "namespaceSelector": {
                    "type": "array",
                    "description": "Namespace selector"
                },
                "additionalLabels": {
                    "type": "object",
                    "description": "Additional labels to add to the ServiceMonitor"
                },
                "jobLabel": {
                    "type": "string",
                    "description": "Job label"
                },
                "targetLabels": {
                    "type": "array",
                    "description": "Target labels",
                    "items": {
                        "type": "string"
                    }
                },
                "podTargetLabels": {
                    "type": "array",
                    "description": "Pod target labels",
                    "items": {
                        "type": "string"
                    }
                },
                "metricRelabelings": {
                    "type": "array",
                    "description": "Metric relabelings"
                },
                "relabelings": {
                    "type": "array",
                    "description": "Relabel configs"
                },
                "multipleTarget": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean",
                            "description": "Enable multi target scraping"
                        },
                        "targets": {
                            "type": "array",
                            "description": "Target connection information",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "endpoint": {
                                        "type": "string",
                                        "description": "MySQL endpoint"
                                    },
                                    "name": {
                                        "type": "string",
                                        "description": "Target name"
                                    },
                                    "port": {
                                        "type": "integer",
                                        "description": "MySQL port"
                                    },
                                    "user": {
                                        "type": "string",
                                        "description": "MySQL user"
                                    },
                                    "password": {
                                        "type": "string",
                                        "description": "MySQL password"
                                    }
                                }
                            }
                        },
                        "sharedSecret": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean",
                                    "description": "Enable shared credentials for all targets"
                                },
                                "name": {
                                    "type": "string",
                                    "description": "Secret name"
                                }
                            }
                        }
                    }
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean",
                    "description": "Specifies whether a ServiceAccount should be created"
                },
                "name": {
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The name of the ServiceAccount to use"
                },
                "annotations": {
                    "type": "object",
                    "description": "ServiceAccount annotations"
                }
            }
        },
        "resources": {
            "type": "object",
            "description": "Resource limits and requests for the container"
        },
        "nodeSelector": {
            "type": "object",
            "description": "Node selector for pod assignment"
        },
        "tolerations": {
            "type": "array",
            "description": "Tolerations for pod assignment"
        },
        "affinity": {
            "type": "object",
            "description": "Affinity for pod assignment"
        },
        "podLabels": {
            "type": "object",
            "description": "Labels to add to the pod"
        },
        "priorityClassName": {
            "type": "string",
            "description": "Priority class name"
        },
        "extraVolumeMounts": {
            "type": "array",
            "description": "Extra volume mounts for the mysql exporter container",
            "items": {
                "type": "object"
            }
        },
        "extraVolumes": {
            "type": "array",
            "description": "Extra volumes for the pod",
            "items": {
                "type": "object"
            }
        },
        "extraEnvs": {
            "oneOf": [
                {
                    "type": "array",
                    "description": "Extra environment variables as array of env var definitions",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "description": "Environment variable name"
                            },
                            "value": {
                                "type": "string",
                                "description": "Environment variable value"
                            },
                            "valueFrom": {
                                "type": "object",
                                "description": "Source for the environment variable value"
                            }
                        }
                    }
                },
                {
                    "type": "object",
                    "description": "Extra environment variables as key-value map",
                    "additionalProperties": {
                        "type": "string"
                    }
                }
            ]
        },
        "podSecurityContext": {
            "type": "object",
            "description": "Pod security context"
        },
        "securityContext": {
            "type": "object",
            "description": "Container security context"
        },
        "annotations": {
            "type": "object",
            "description": "Annotations to add to the pod"
        },
        "config": {
            "type": "object",
            "properties": {
                "logLevel": {
                    "type": "string",
                    "description": "Log level"
                },
                "logFormat": {
                    "type": "string",
                    "description": "Log format"
                },
                "extraArgs": {
                    "type": "array",
                    "description": "Extra arguments as a list of strings",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "collectors": {
            "type": "object",
            "description": "Collector configuration options. Values can be boolean (enable/disable collector), string, or integer",
            "additionalProperties": {
                "oneOf": [
                    {
                        "type": "boolean"
                    },
                    {
                        "type": "string"
                    },
                    {
                        "type": "integer"
                    }
                ]
            }
        },
        "mysql": {
            "type": "object",
            "properties": {
                "db": {
                    "type": "string",
                    "description": "MySQL database name"
                },
                "host": {
                    "type": "string",
                    "description": "MySQL host"
                },
                "additionalConfig": {
                    "oneOf": [
                        {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "Additional my.cnf config options"
                },
                "pass": {
                    "type": "string",
                    "description": "MySQL password"
                },
                "port": {
                    "type": "integer",
                    "description": "MySQL port"
                },
                "protocol": {
                    "type": "string",
                    "description": "MySQL protocol"
                },
                "user": {
                    "type": "string",
                    "description": "MySQL user"
                },
                "existingConfigSecret": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string",
                            "description": "Secret name with full my.cnf config"
                        },
                        "key": {
                            "type": "string",
                            "description": "Secret key"
                        }
                    }
                },
                "existingPasswordSecret": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string",
                            "description": "Secret name containing the password"
                        },
                        "key": {
                            "type": "string",
                            "description": "Secret key"
                        }
                    }
                }
            }
        },
        "cloudsqlproxy": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "description": "Enable Cloud SQL proxy sidecar"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "repo": {
                            "type": "string",
                            "description": "Cloud SQL proxy image repository"
                        },
                        "tag": {
                            "type": "string",
                            "description": "Cloud SQL proxy image tag"
                        },
                        "pullPolicy": {
                            "type": "string",
                            "description": "Cloud SQL proxy image pull policy",
                            "enum": [
                                "Never",
                                "IfNotPresent",
                                "Always"
                            ]
                        }
                    }
                },
                "instanceConnectionName": {
                    "type": "string",
                    "description": "Cloud SQL instance connection name (project:region:instance)"
                },
                "privateIp": {
                    "type": "boolean",
                    "description": "Use private IP for Cloud SQL connection"
                },
                "port": {
                    "type": "string",
                    "description": "Cloud SQL proxy port"
                },
                "credentialsSecret": {
                    "type": "string",
                    "description": "Secret containing Cloud SQL credentials"
                },
                "credentials": {
                    "type": "string",
                    "description": "Service account JSON credentials"
                },
                "workloadIdentity": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean",
                            "description": "Enable workload identity"
                        },
                        "serviceAccountEmail": {
                            "type": "string",
                            "description": "Service account email"
                        }
                    }
                },
                "extraArgs": {
                    "type": "string",
                    "description": "Extra arguments for Cloud SQL proxy"
                }
            }
        },
        "prometheusRule": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "description": "Enable custom PrometheusRules"
                },
                "additionalLabels": {
                    "type": "object",
                    "description": "Additional labels for PrometheusRule"
                },
                "namespace": {
                    "type": "string",
                    "description": "Namespace for PrometheusRule"
                },
                "rules": {
                    "type": "array",
                    "description": "PrometheusRule rules",
                    "items": {
                        "type": "object"
                    }
                }
            }
        },
        "extraManifests": {
            "type": "array",
            "description": "Extra manifests to deploy as an array. Supports both string format (with pipe |) and object format",
            "items": {
                "oneOf": [
                    {
                        "type": "string",
                        "description": "Manifest as YAML string"
                    },
                    {
                        "type": "object",
                        "description": "Manifest as structured object",
                        "properties": {
                            "apiVersion": {
                                "type": "string"
                            },
                            "kind": {
                                "type": "string"
                            },
                            "metadata": {
                                "type": "object"
                            }
                        }
                    }
                ]
            }
        }
    }
}

